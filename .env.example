# Development Mode
# Set to "true" or "1" to enable development mode
DEV_MODE=false

# PostgreSQL Database Configuration
POSTGRES_USER=your_postgres_user
POSTGRES_PASSWORD=your_postgres_password
POSTGRES_HOST=localhost
POSTGRES_OUTPUT_PORT=5432
POSTGRES_DB=your_database_name

# PostgreSQL SSH Configuration
# Set to "true" or "1" if SSH tunnel is required for database connection
POSTGRES_SSH_REQUIRED=false

# Google Gemini AI API
GEMIN_API_KEY=your_gemini_api_key

# LinkedIn Credentials
LINKEDIN_LOGIN=your_linkedin_email@example.com
LINKEDIN_PASSWORD=your_linkedin_password

# Instagram Credentials
IG_LOGIN=your_instagram_username
IG_PASSWORD=your_instagram_password
IG_USERNAME=your_instagram_username

# CI Environment (automatically set by CI systems)
# CI=true
# NODE_ENV=production